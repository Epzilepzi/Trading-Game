{
  "name": "spree",
  "version": "1.0.0",
  "description": "Spree - The totally accurate ultra realistic trading game.",
  "main": "main.js",
  "repository": "https://github.com/Epzilepzi/Trading-Game",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "build": "build --mac --linux --win",
    "ship": "build --mac --linux -p always",
    "appImage": "build --linux appImage",
    "buildWin": "build --win",
    "shipWin": "build --win -p always"
  },
  "author": {
    "name": "Epzilepzi",
    "email": "epzilepzi@gmail.com",
    "url": "https://epzilepzi.site"
  },
  "license": "ISC",
  "devDependencies": {
    "ajv": "^6.5.4",
    "asar": "^0.14.3",
    "electron": "^2.0.10",
    "electron-builder": "^20.28.4"
  },
  "build": {
    "productName": "Spree",
    "appId": "com.yeetmeister.spree",
    "win": {
      "target": "portable",
      "icon": "icons/icon.ico"
    },
    "mac": {
      "category": "public.app-category.games",
      "icon": "icons/icon.icns"
    },
    "linux": {
      "target": "appImage",
      "icon": "icons/icon.icns"
    }
  },
  "dependencies": {
    "electron-updater": "^3.1.2",
    "jquery": "^3.3.1",
    "jshint": "^2.9.6"
  }
}
